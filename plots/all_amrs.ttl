# Define entities Venezuela, Other places, Asuncion, Paraguay, Four Mayors, "They"
:Venezuela a whata:Blamable .

:OtherPlaces a whata:CounterBlamable ;
    per:shotThrough :WhataboutistLens_1 .

:Asuncion a whata:BlameSituation .

:Paraguay a whata:BlameSituation .

:FourMayors a whata:BlameSituation .

:They a whata:ResponsibilityOwner ;
    whata:isParticipantIn :Venezuela ;
    whata:isParticipantIn :OtherPlaces ;
    whata:isParticipantIn :blended_resp_country.

# Define the Whataboutist Perspectivisation
:WhataboutistPerspectivisation_14 a whata:WhataboutistPerspectivisation ;
    whata:relatedIdentifyingPattern "but there are many other places" ;
    per:Lens :WhataboutistLens_1 ;
    per:Cut :OtherPlaces ;
    per:Background :blended_resp_country ;
    per:Eventuality :Venezuela .

# Define the Whataboutist Lens
:WhataboutistLens_1 a whata:WhataboutistLens ;
    whata:shiftsFocusFrom :Venezuela .

# Define the blended blame background
:blended_resp_country a whata:BlendedBlameBackground ;
    per:Background :BackgroundEvent .

# Define the moral judgment event
:MoralJudgment a whata:Ought ;
    whata:isParticipantIn :Venezuela ;
    whata:isParticipantIn :OtherPlaces ;
    whata:isParticipantIn :blended_resp_country.

# Define the background event (the context of the blended blame)
:BackgroundEvent a pbdata:blame-01 ;
    pbdata:actor :They ;
    pbdata:action_target :Venezuela, :OtherPlaces .

# Define the criticism event
:CriticismEvent a pbdata:criticize-01 ;
    pbdata:critic :They ;
    pbdata:entity_being_criticized :Venezuela, :OtherPlaces .

# Define the killing event
:KillingEvent a pbdata:kill-01 ;
    pbdata:killer :They ;
    pbdata:entity_killed :FourMayors ;
    pbdata:location :Asuncion, :Paraguay .

# Define 'They' as a collective entity
:They a whata:ResponsibilityOwner ;
    whata:isParticipantIn :CriticismEvent ;
    whata:isParticipantIn :KillingEvent ;
    whata:isParticipantIn :blended_resp_country.


# Define entities alt-right, alt-left, ResponsibilityOwner1, ResponsibilityOwner2, clubs, problem, guilt, and "they"
:AltRight a whata:Blamable .
   
    
    

:AltLeft a whata:CounterBlamable ;
    
    per:shotThrough :WhataboutistLens_1 .

:ResponsibilityOwner1 a whata:ResponsibilityOwner ;
    whata:isParticipantIn :AltRight ;
    whata:isParticipantIn :blended_r_politics.

:ResponsibilityOwner2 a whata:ResponsibilityOwner ;
    whata:isParticipantIn :AltLeft ;
    whata:isParticipantIn :blended_r_politics.

:Clubs a whata:BlameSituation .

:Problem a whata:BlameSituation .

:Guilt a whata:BlameSituation .

# Define the Whataboutist Perspectivisation
:WhataboutistPerspectivisation_3 a whata:WhataboutistPerspectivisation ;
    whata:relatedIdentifyingPattern "what about the alt-left" ;
    per:Lens :WhataboutistLens_1 ;
    per:Cut :AltLeft ;
    per:Background :blended_r_politics;
    per:Eventuality :AltRight .

# Define the Whataboutist Lens
:WhataboutistLens_1 a whata:WhataboutistLens ;
    whata:shiftsFocusFrom :AltRight .

# Define the blended blame background
:blended_r_politics a whata:BlendedBlameBackground .

# Define the oughts (norms or duties being infringed)
:OughtAltRight a whata:Ought ;
    whata:isParticipantIn :blended_r_politics ;
    whata:isParticipantIn :AltRight .

:OughtAltLeft a whata:Ought ;
    whata:isParticipantIn :blended_r_politics ;
    whata:isParticipantIn :AltLeft .

# Define the charging event
:ChargingEvent a pbdata:charge-01 ;
    pbdata:charger :They ;
    pbdata:entity_being_charged :AltLeft ;
    pbdata:weapon_used :Clubs ;
    pbdata:location :BlameSituation .

# Define the guilt event
:GuiltEvent a pbdata:guilt-01 ;
    pbdata:guilty_party :They ;
    pbdata:guilt_target :AltRight ;
    pbdata:questioned :Problem .

# Define the problem event
:ProblemEvent a pbdata:problem-01 ;
    pbdata:problem_entity :They ;
    pbdata:problem_target :AltRight .

# Define 'They' as a collective entity
:They a whata:ResponsibilityOwner ;
    whata:isParticipantIn :ChargingEvent ;
    whata:isParticipantIn :GuiltEvent ;
    whata:isParticipantIn :ProblemEvent .



 
:We a whata:Blamable .
    

:YourSide a whata:CounterBlamable ;
    
    per:shotThrough :WhataboutistLens_2 .

:ResponsibilityOwner1 a whata:ResponsibilityOwner ;
    whata:isParticipantIn :We ;
