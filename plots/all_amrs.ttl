    whata:relatedIdentifyingPattern "but 'your side' does shitty things too" ;
    per:Lens :WhataboutistLens_2 ;
    per:Cut :YourSide ;
    per:Background :blended_r_party ;
    per:Eventuality :We .

# Define the Whataboutist Lens
:WhataboutistLens_2 a whata:WhataboutistLens ;
    whata:shiftsFocusFrom :YourSide .

# Define the blended blame background
:blended_r_party a whata:BlendedBlameBackground .

# Define the oughts (norms or duties being infringed)
:OughtWe a whata:Ought ;
    whata:isParticipantIn :blended_r_party;
    whata:isParticipantIn :We .

:OughtYourSide a whata:Ought ;
    whata:isParticipantIn :blended_r_party ;
    whata:isParticipantIn :YourSide .

# Define 'They' as a collective entity (mentioned in the background)
:They a whata:ResponsibilityOwner ;
    whata:isParticipantIn :blended_r_party .



