@prefix amr: <http://www.ontologydesignpatterns.org/ont/amr/> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix fnframe: <https://w3id.org/framester/framenet/abox/frame/> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
@prefix fschema: <https://w3id.org/framester/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pbdata: <https://w3id.org/framester/pb/data/> .
@prefix pblr: <https://w3id.org/framester/pb/localrole/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix va: <http://verbatlas.org/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix wn30: <https://w3id.org/framester/wn/wn30/instances/> .
@prefix wn30schema: <https://w3id.org/framester/wn/wn30/schema/> .



@prefix whata: <https://raw.githubusercontent.com/Salvadana/whata_ontology/main/ontology_development/whata_ontology.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix per: <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#> .



whata:perspectivisation_2 a whata:WhataboutistPerspectivisation;     
    whata:relatedIdentifyingPattern 'the root of the climate change problem';
    per:Eventuality fred:produce_1;
    per:Cut fred:consume_1;
    per:Lens fred:contrast_1;
    per:Background fred:problem_1.



fred:contrast_1 a pbdata:contrast-01 ;
   a whata:WhataboutistLens;
    whata:shiftsFocusFrom fred:produce_1;
    pblr:first_item_in_comparison fred:consume_1 ;
    pblr:second_item_in_comparison fred:produce_1 ;
    fschema:subsumedUnder va:0627f .

fred:guide_1 a pbdata:guide-01 ;
    pblr:guide fred:principle_1 ;
    fschema:subsumedUnder va:0387f,
        fnframe:Cotheme,
        fnframe:Influence_of_event_on_cognizer .

fred:predicate_1 a pbdata:predicate-01 ;
    

    pblr:entity_predicated fred:principle_1 ;
    pblr:predicated_on fred:Idea ;
    fschema:subsumedUnder va:0080f .

fred:root_1 a fred:Idea,
        pbdata:root-02 ;
    pblr:rooted_in_what fred:consume_1 ;
    pblr:thing_having_roots fred:problem_1 ;
    fschema:subsumedUnder va:0624f .

fred:Climate rdfs:subClassOf dul:Situation,
        wn30:supersense-noun_state ;
    owl:equivalentClass wn30:synset-climate-noun-1 .

fred:Fossil rdfs:subClassOf dul:Quality,
        wn30:supersense-adj_pert ;
    owl:equivalentClass wn30:synset-fossil-adjective-1 .

fred:FossilFuel dul:associatedWith fred:Fossil ;
    rdfs:subClassOf fred:Fuel .

fred:Fuel rdfs:subClassOf dul:PhysicalObject,
        wn30:supersense-noun_substance ;
    owl:equivalentClass wn30:synset-fuel-noun-1 .

fred:Principle rdfs:subClassOf dul:InformationEntity,
        wn30:supersense-noun_cognition ;
    owl:equivalentClass wn30:synset-principle-noun-2 .

fred:Problem rdfs:subClassOf dul:Situation,
        wn30:supersense-noun_state ;
    owl:equivalentClass wn30:synset-problem-noun-1 .

fred:Yale a amr:University ;
    a whata:ResponsibilityOwner;
    whata:actsAgainst fred:climate_1;
    whata:isParticipantIn fred:consume_1 ;
    whata:isParticipantIn fred:produce_1 ;
    whata:isParticipantIn fred:problem_1 ;
    owl:sameAs dbpedia:Yale_University,
        wd:Q49112 .

fred:change_1 a pbdata:change-01 ;
    pblr:thing_changing fred:climate_1 ;
    fschema:subsumedUnder va:0016f,
        fnframe:Cause_change,
        fnframe:Undergo_change .

fred:climate_1 a fred:Climate ;
    a whata:Ought;
    whata:isParticipantIn fred:consume_1 ;
    whata:isParticipantIn fred:produce_1 ;
    whata:isParticipantIn fred:problem_1 .

fred:problem_1 a fred:Problem ;
    a whata:BlendedBlameBackground;
    fred:about fred:change_1 .

fred:produce_1 a pbdata:produce-01 ;
    a whata:Blamable;

    pblr:creation fred:fuel_1 ;
    fschema:subsumedUnder va:0483f,
        fnframe:Causation,
        fnframe:Cause_to_start,
        fnframe:Creating,
        fnframe:Intentionally_create,
        fnframe:Manufacturing .

pbdata:change-01 rdfs:label "transform" ;
    rdfs:subClassOf dul:Event .

pbdata:consume-01 rdfs:label "eat, use up, devouring" ;
    rdfs:subClassOf dul:Event .

pbdata:contrast-01 rdfs:label "point out differences, measure of difference" ;
    rdfs:subClassOf dul:Event .

pbdata:guide-01 rdfs:label "to direct" ;
    rdfs:subClassOf dul:Event .

pbdata:predicate-01 rdfs:label "to base or establish" ;
    rdfs:subClassOf dul:Event .

pbdata:produce-01 rdfs:label "create, accomplishing" ;
    rdfs:subClassOf dul:Event .

pbdata:root-02 rdfs:label "base, justify" ;
    rdfs:subClassOf dul:Event .

wn30:synset-climate-noun-1 wn30schema:gloss "the weather in some location averaged over some long period of time; \"the dank climate of southern Wales\"; \"plants from a cold clime travel best in winter\""@en-us .

wn30:synset-fossil-adjective-1 wn30schema:gloss "characteristic of a fossil"@en-us .

wn30:synset-fuel-noun-1 wn30schema:gloss "a substance that can be consumed to produce energy; \"more fuel is needed during the winter months\"; \"they developed alternative fuels for aircraft\""@en-us .

wn30:synset-idea-noun-1 wn30schema:gloss "the content of cognition; the main thing you are thinking about; \"it was not a good idea\"; \"the thought never entered my mind\""@en-us .

wn30:synset-principle-noun-2 wn30schema:gloss "a rule or standard especially of good behavior; \"a man of principle\"; \"he will not violate his principles\""@en-us .

wn30:synset-problem-noun-1 wn30schema:gloss "a state of difficulty that needs to be resolved; \"she and her husband are having problems\"; \"it is always a job to contact him\"; \"urban problems such as traffic congestion and smog\""@en-us .

fred:Idea rdfs:subClassOf dul:InformationEntity,
        wn30:supersense-noun_cognition ;
    owl:equivalentClass wn30:synset-idea-noun-1 .

fred:consume_1 a pbdata:consume-01 ;
    a whata:CounterBlamable;
    per:shotThrough fred:contrast_1;

    pblr:consumed fred:fuel_1 ;
    fschema:subsumedUnder va:0135f,
        fnframe:Expend_resource,
        fnframe:Ingestion .

fred:fuel_1 a fred:FossilFuel .

fred:principle_1 a fred:Principle ;
    fred:principleOf fred:Yale .

