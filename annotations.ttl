@prefix ex: <http://example.org/annotations#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:Billy a ex:ResponsibilityOwner ;
    rdfs:label "Billy" .

ex:Bobby a ex:ResponsibilityOwner ;
    rdfs:label "Bobby" .

ex:What_about_Bobby a ex:WhataboutistPerspectivisation ;
    rdfs:label "What about Bobby" .

ex:room a ex:BlamableEventuality ;
    rdfs:label "room" .

ex:tidy_your_room a ex:Ought ;
    rdfs:label "tidy your room" .

ex:BlamableEventuality rdfs:label "BlamableEventuality" .

ex:Ought rdfs:label "Ought" .

ex:WhataboutistPerspectivisation rdfs:label "WhataboutistPerspectivisation" .

ex:ResponsibilityOwner rdfs:label "ResponsibilityOwner" .

