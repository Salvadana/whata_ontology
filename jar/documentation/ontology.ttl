###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#actsAgainst
:actsAgainst rdf:type owl:ObjectProperty ;
             rdfs:domain :ResponsibilityOwner ;
             rdfs:range :MoralValue .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#blames
:blames rdf:type owl:ObjectProperty ;
        rdfs:domain :BlamingClaim ;
        rdfs:range :BlamableEventuality .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#hasLinguisticIdentifyingPattern
:hasLinguisticIdentifyingPattern rdf:type owl:ObjectProperty ;
                                 owl:inverseOf :isLinguisticIdentifyingPatternOf ;
                                 rdfs:domain :Whataboutism ;
                                 rdfs:range :LinguisticIdentifyingPattern .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#isBasedOn
:isBasedOn rdf:type owl:ObjectProperty ;
           rdfs:domain :Whataboutism ;
           rdfs:range :BlamingClaimSituation .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#isContrastedIn
:isContrastedIn rdf:type owl:ObjectProperty ;
                rdfs:domain :MoralValue ;
                rdfs:range :BlamableEventuality .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#isLinguisticIdentifyingPatternOf
:isLinguisticIdentifyingPatternOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#isObservedWith
:isObservedWith rdf:type owl:ObjectProperty ;
                rdfs:domain :BlamableEventuality ;
                rdfs:range :FocusShiftingLens .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :ConfirmationBias ,
                       :RelevanceFallacy ;
           rdfs:range :Whataboutism .


###  http://www.semanticweb.org/lored/ontologies/2024/4/untitled-ontology-25#shiftsFocusOn
:shiftsFocusOn rdf:type owl:ObjectProperty ;
               rdfs:domain :FocusShiftingLens ;
               rdfs:range :BlamableEventuality .


#################################################################
#    Classes
